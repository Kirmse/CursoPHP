algoritmo "semnome"

var
valor : inteiro
inicio
escreval("Testo do número 10")
escreval("-------------------")
escreval("")
escreva("Coloque o numero aqui: ")
leia(valor)
escreval("")

se (valor > 10) entao
   escreval("Este valor é maior que 10")
senao
   escreval("Este valor não é maior que 10")
fimse
fimalgoritmo
